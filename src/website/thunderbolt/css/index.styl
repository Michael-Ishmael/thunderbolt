@import './reset'
@import './mixin'

.stack > *:not(.active) {
  display: none
}

#app
    width: 100%
    min-height: 200px

#navigation
    float: right

#main
    vbg-gradient(#aaa, tc = #666)

.clear
    clear: both

ul.navigation
    padding 0px

ul.navigation li
    display block
    float left
    margin 10px
    padding 5px
    background-color #f6f6f6
    border 1px solid #333
    cursor pointer

